{
  "use_weight_map": true,
  "export_inputs": false,
  "write_graph": true,
  "inputs": {
    "use_fp16": false,
    "subject_batch_size": 2,
    "batch_size": 8,
    "eval_batch_size": 8,
    "n_modalities": 2,
    "patch_size": 64,
    "dataset": {
      "iSEG": {
        "volume": {
          "width": 144,
          "height": 192,
          "depth": 256,
          "n_channels": 1
        }
      },
      "MRBrainS": {
        "volume": {
          "width": 250,
          "height": 280,
          "depth": 240,
          "n_channels": 1
        }
      }
    }
  },
  "model": {
    "batch_norm_decay": 0.997,
    "batch_norm_epsilon": 1e-5,
    "weight_decay": 2e-4,
    "learning_rate": 0.0001,
    "learning_rate_decay": 0.995,
    "n_epochs": 1000000,
    "n_filters": [
      16,
      32,
      64,
      128,
      256
    ],
    "activation_func": "relu",
    "n_classes": 4,
    "log_interval": 1
  }
}